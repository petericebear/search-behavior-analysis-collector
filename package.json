{
  "name": "search-behavior-analysis-collector",
  "version": "0.1.0",
  "description": "Custom JavaScript tracker for user behavior and click tracking",
  "main": "src/tracker.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src __tests__",
    "lint:fix": "eslint src __tests__ --fix",
    "format": "prettier --write \"src/**/*.js\" \"__tests__/**/*.js\""
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@eslint/js": "^9.39.2",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "globals": "^16.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.2.5",
    "terser-webpack-plugin": "^5.3.16",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
} 